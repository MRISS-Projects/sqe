<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.apisupport.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/nb-module-project/3">
            <code-name-base>org.nbheaven.sqe.wrapper.antlr.runtime</code-name-base>
            <suite-component/>
            <module-dependencies/>
            <public-packages>
                <package>org.antlr.v4.runtime</package>
                <package>org.antlr.v4.runtime.atn</package>
                <package>org.antlr.v4.runtime.dfa</package>
                <package>org.antlr.v4.runtime.misc</package>
                <package>org.antlr.v4.runtime.tree</package>
                <package>org.antlr.v4.runtime.tree.gui</package>
                <package>org.antlr.v4.runtime.tree.pattern</package>
                <package>org.antlr.v4.runtime.tree.xpath</package>
            </public-packages>
            <class-path-extension>
                <runtime-relative-path>ext/antlr4-runtime.jar</runtime-relative-path>
                <binary-origin>release/modules/ext/antlr4-runtime.jar</binary-origin>
            </class-path-extension>
            <class-path-extension>
                <runtime-relative-path>ext/antlr4-annotations.jar</runtime-relative-path>
                <binary-origin>release/modules/ext/antlr4-annotations.jar</binary-origin>
            </class-path-extension>
        </data>
    </configuration>
</project>
