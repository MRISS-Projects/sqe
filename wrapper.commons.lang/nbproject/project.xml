<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.apisupport.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/nb-module-project/3">
            <code-name-base>org.nbheaven.sqe.wrapper.commons.lang</code-name-base>
            <suite-component/>
            <module-dependencies/>
            <test-dependencies>
                <test-type>
                    <name>unit</name>
                    <test-dependency>
                        <code-name-base>org.netbeans.libs.junit4</code-name-base>
                        <compile-dependency/>
                    </test-dependency>
                </test-type>
            </test-dependencies>
            <public-packages>
                <package>org.apache.commons.lang</package>
                <package>org.apache.commons.lang.builder</package>
                <package>org.apache.commons.lang.enums</package>
                <package>org.apache.commons.lang.exception</package>
                <package>org.apache.commons.lang.math</package>
                <package>org.apache.commons.lang.mutable</package>
                <package>org.apache.commons.lang.text</package>
                <package>org.apache.commons.lang.time</package>
            </public-packages>
            <class-path-extension>
                <runtime-relative-path>ext/commons-lang.jar</runtime-relative-path>
                <binary-origin>release/modules/ext/commons-lang.jar</binary-origin>
            </class-path-extension>
        </data>
    </configuration>
</project>
