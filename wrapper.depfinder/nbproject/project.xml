<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.apisupport.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/nb-module-project/3">
            <code-name-base>org.nbheaven.sqe.wrapper.depfinder</code-name-base>
            <suite-component/>
            <module-dependencies>
                <dependency>
                    <code-name-base>org.nbheaven.sqe.wrapper.log4j</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>1.0</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.nbheaven.sqe.wrapper.jakarta.oro</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>2.0.8</specification-version>
                    </run-dependency>
                </dependency>
            </module-dependencies>
            <public-packages>
                <package>com.jeantessier.classreader</package>
                <package>com.jeantessier.commandline</package>
                <package>com.jeantessier.dependency</package>
                <package>com.jeantessier.dependencyfinder</package>
                <package>com.jeantessier.dependencyfinder.ant</package>
                <package>com.jeantessier.dependencyfinder.cli</package>
                <package>com.jeantessier.dependencyfinder.gui</package>
                <package>com.jeantessier.dependencyfinder.web</package>
                <package>com.jeantessier.dependencyfinder.webwork</package>
                <package>com.jeantessier.diff</package>
                <package>com.jeantessier.fit</package>
                <package>com.jeantessier.fitlibrary</package>
                <package>com.jeantessier.metrics</package>
                <package>com.jeantessier.text</package>
                <package>otherpackage</package>
                <package>sloc</package>
                <package>testpackage</package>
            </public-packages>
            <class-path-extension>
                <runtime-relative-path>ext/DependencyFinder.jar</runtime-relative-path>
                <binary-origin>release/modules/ext/DependencyFinder.jar</binary-origin>
            </class-path-extension>
        </data>
    </configuration>
</project>
