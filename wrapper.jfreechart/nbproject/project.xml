            <class-path-extension>
                <runtime-relative-path>ext/jfreechart.jar</runtime-relative-path>
                <binary-origin>release/modules/ext/jfreechart.jar</binary-origin>
            </class-path-extension>
            <class-path-extension>
                <runtime-relative-path>ext/jfreechart-experimental.jar</runtime-relative-path>
                <binary-origin>release/modules/ext/jfreechart-experimental.jar</binary-origin>
            </class-path-extension>
